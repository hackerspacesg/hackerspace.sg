<div class="row">
  <div class="col-md-6 events-block">
    <h2>Upcoming Events</h2>
    <ul>
      {{#events}}
        <li>
          {{#URL}}<h4><a href="{{URL}}">{{Name}}</a></h4>{{/URL}}
          {{^URL}}<h4>{{Name}}</h4>{{/URL}}
          <time dateTime="{{When}}" title="{{When}}">{{When}}</time>
        </li>
      {{/events}}
    </ul>
  </div>
  <div class="col-md-6">
    <iframe src="/tweets.html" scrolling="no" frameborder="0" allowtransparency="true" width="520" height="600" style="margin-top: 27px; border: none; max-width: 100%; min-width: 180px;"></iframe>
  </div>
</div>

{{> footer }}
